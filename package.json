{
    "name": "cancer-nft",
    "scripts": {
        "compile": "npx hardhat compile",
        "deploy:localhost": "npx hardhat run scripts/deploy.js --network hardhat",
        "deploy:ropsten": "npx hardhat run scripts/deploy.js --network ropsten",
        "deploy:mainnet": "npx hardhat run scripts/deploy.js --network mainnet",
        "devchain": "npx hardhat node",
        "prettier": "prettier --write 'contracts/**/*.sol' '*/**/{*.js,*.json}' '*.js' '*.json'"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "2.0.2",
        "@nomiclabs/hardhat-etherscan": "2.1.7",
        "@nomiclabs/hardhat-waffle": "2.0.1",
        "chai": "4.3.4",
        "ethereum-waffle": "3.4.0",
        "ethers": "5.5.1",
        "hardhat": "2.6.7",
        "prettier": "2.5.1",
        "prettier-plugin-solidity": "1.0.0-beta.19"
    },
    "dependencies": {
        "@openzeppelin/contracts": "4.4.2"
    }
}
